      }
    }
  },
  "defaultProject": "devto",
  "cli": {
    "analytics": "dc7499a8-1f6f-441e-8c70-2331aa336fba",
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "buildable": true
    },
    "@nrwl/angular:component": {}
  }
