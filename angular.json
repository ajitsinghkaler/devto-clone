{
  "version": 1,
  "projects": {
    "core-models": {
      "projectType": "library",
      "root": "libs/core/models",
      "sourceRoot": "libs/core/models/src",
      "prefix": "devto",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/core/models"],
          "options": {
            "project": "libs/core/models/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/core/models/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/core/models/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core/models"],
          "options": {
            "jestConfig": "libs/core/models/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core/models/src/**/*.ts",
              "libs/core/models/src/**/*.html"
            ]
          }
        }
      }
    },
    "devto": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/devto",
      "sourceRoot": "apps/devto/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/devto",
            "index": "apps/devto/src/index.html",
            "main": "apps/devto/src/main.ts",
            "polyfills": "apps/devto/src/polyfills.ts",
            "tsConfig": "apps/devto/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/devto/src/favicon.ico", "apps/devto/src/assets"],
            "styles": ["apps/devto/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "libs/environments/src/lib/environment.ts",
                  "with": "libs/environments/src/lib/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "namedChunks": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "devto:build:production"
            },
            "development": {
              "browserTarget": "devto:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "devto:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/devto/src/test.ts",
            "polyfills": "apps/devto/src/polyfills.ts",
            "tsConfig": "apps/devto/tsconfig.spec.json",
            "karmaConfig": "apps/devto/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/devto/src/favicon.ico", "apps/devto/src/assets"],
            "styles": ["apps/devto/src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/devto/src/**/*.ts",
              "apps/devto/src/**/*.html"
            ]
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    },
    "environments": {
      "projectType": "library",
      "root": "libs/environments",
      "sourceRoot": "libs/environments/src",
      "prefix": "devto",
      "architect": {
